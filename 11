#include <stdio.h>
#include <stdlib.h>

int main() {
    int i = 0;
    int size;
    char a[80];
    int* b = malloc(size * sizeof(int));
    fgets(a, sizeof(a), stdin);
    while (*(a+i) != ' ')
        i++;
        *(b+i) = *(a+i);
    for (i == 0; i < size; i++)
        printf("%s", b+i);
    return 0;
}


