#include <stdio.h>


void prost(int n) {
    if (int i == 1)
    return 0;
    int k = 0;
    for (i = 2; i < pow(n,0.5); i++)
        if (n % i == 0)
            k++
    if (k == 2)
        return 1;
        

int main
}
